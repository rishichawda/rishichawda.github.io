    "format": "prettier \"**/*.{md,mdx,yml,json} \" --write",
    "lint": "eslint . --ext .js,.jsx --fix",
    "test:lhs": "mocha ./tests/lighthouse/lighthouse.test.js",
    "test:lhs:server": "server-test serve :9000 test:lhs",
